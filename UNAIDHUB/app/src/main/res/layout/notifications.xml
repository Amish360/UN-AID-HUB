<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:background="@drawable/frame">

    <ImageView android:id="@+id/vector"
        android:src="@drawable/vector__1_"
        android:contentDescription="vector"
        android:layout_marginStart="10dp"
        android:layout_marginTop="0.25dp"
        android:layout_width="18dp"
        android:layout_height="22.5dp" />

    <TextView android:id="@+id/notification_header"
        android:layout_gravity="center_horizontal|center"
        android:gravity="center"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="0dp"
        android:layout_width="82.4dp"
        android:layout_height="41.4dp"
        android:text="@string/notification"
        />


    <View
        android:id="@+id/_bg__frame"
        android:layout_width="302dp"
        android:layout_height="86dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="51dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="58dp"
        android:layout_marginBottom="493dp"
        android:background="@drawable/rectangle_5" />

    <TextView
        android:id="@+id/date_header_1"
        android:layout_width="132.2dp"
        android:layout_height="20.2dp"
        android:layout_alignTop="@+id/_bg__frame"
        android:layout_alignEnd="@+id/_bg__frame"
        android:layout_alignBottom="@+id/_bg__frame"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="162dp"
        android:layout_marginBottom="59dp"
        android:text="@string/Today"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/news1"
        android:layout_width="288.24dp"
        android:layout_height="40.4dp"
        android:layout_alignTop="@+id/_bg__frame"
        android:layout_alignEnd="@+id/_bg__frame"
        android:layout_alignBottom="@+id/_bg__frame"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="10dp"
        android:text="@string/select_cont"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:textStyle="bold" />


    <View
        android:id="@+id/_bg__frame2"
        android:layout_width="302dp"
        android:layout_height="86dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="54dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="388dp"
        android:background="@drawable/rectangle_5" />

    <TextView
        android:id="@+id/date_header_2"
        android:layout_width="132.2dp"
        android:layout_height="20.2dp"
        android:layout_above="@+id/_bg__frame2"
        android:layout_alignTop="@+id/_bg__frame2"
        android:layout_alignEnd="@+id/_bg__frame2"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="167dp"
        android:layout_marginBottom="-24dp"
