        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/news2"
        android:layout_width="288.24dp"
        android:layout_height="40.4dp"
        android:layout_alignTop="@+id/_bg__frame2"
        android:layout_alignEnd="@+id/_bg__frame2"
        android:layout_alignBottom="@+id/_bg__frame2"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="11dp"
